{
  "name": "kintone-barcodeScanner",
  "version": "0.1.0",
  "scripts": {
    "start": "node scripts/npm-start.js",
    "develop": "npm run build -- --watch",
    "build": "kintone-plugin-packer --ppk private.ppk --out dist/plugin.zip src",
    "lint": "eslint src",
    "upload": "kintone-plugin-uploader dist/plugin.zip --watch --waiting-dialog-ms 3000"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^23.0.0",
    "@kintone/plugin-packer": "^8.1.2",
    "@kintone/plugin-uploader": "^9.1.2",
    "@kintone/webpack-plugin-kintone-plugin": "^8.0.9",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@ericblade/quagga2": "^1.8.4",
    "jquery": "^3.7.1",
    "kintone-ui-component": "^1.17.1",
    "quagga": "^0.12.1"
  }
}
